# Plot 

plot_enrichment <- function() {
  
  baseline <- data.frame(x = 0, y = 0, xend = xaxis_max, yend = 0)
  
  p <- use_pathway %>%
    ggplot(aes(-log10(p.adjust), y = index, fill = ONTOLOGY)) +
    geom_round_col(
      aes(y = Description), width = 0.6, alpha = 0.8
    ) +
    geom_text(
      aes(x = 0.05, label = Description),
      hjust = 0, size = 5
    ) +
